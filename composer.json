{
  "name": "kostajh/ac-task",
  "description": "Integration between Taskwarrior and ActiveCollab 3",
  "license": "GPL-2.0+",
  "repositories": [
    {
        "type": "vcs",
        "url": "https://github.com/kostajh/ActiveCollabApi"
    }
  ],
  "require": {
      "symfony/console": "2.*",
      "symfony/yaml": "2.*",
      "symfony/filesystem": "2.*",
      "symfony/process": "2.*",
      "kostajh/ActiveCollabApi": "dev-rewrite",
      "libtask/libtask": "dev-master",
      "doctrine/common": "2.5.*@dev",
      "jms/serializer": "dev-master"
  },
  "autoload": {
      "psr-0": { "AcTask": "src/" }
  },
  "bin": ["ac-task"]
}
